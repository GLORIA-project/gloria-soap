						scope="singleton">
						<property name="operation" ref="Iteration"></property>
						<property name="relations">
							<list>
								<value>load_ccd_name_ptr</value>
								<value>cameras.ccd.order</value>
								<value>cameras.ccd.number</value>
							</list>
						</property>
