<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="credentialPart" class="java.lang.String" abstract="true">
	</bean>

	<bean id="actionLogUsername" parent="credentialPart">
		<constructor-arg value="user" />
	</bean>

	<bean id="actionLogPassword" parent="credentialPart">
		<constructor-arg value="password" />
	</bean>

	<bean id="experimentUsername" parent="credentialPart">
		<constructor-arg value="user" />
	</bean>

	<bean id="experimentPassword" parent="credentialPart">
		<constructor-arg value="password" />
	</bean>

	<bean id="rtRepositoryUsername" parent="credentialPart">
		<constructor-arg value="user" />
	</bean>

	<bean id="rtRepositoryPassword" parent="credentialPart">
		<constructor-arg value="password" />
	</bean>

	<bean id="userRepositoryUsername" parent="credentialPart">
		<constructor-arg value="user" />
	</bean>

	<bean id="userRepositoryPassword" parent="credentialPart">
		<constructor-arg value="password" />
	</bean>

	<bean id="imageRepositoryUsername" parent="credentialPart">
		<constructor-arg value="user" />
	</bean>

	<bean id="imageRepositoryPassword" parent="credentialPart">
		<constructor-arg value="password" />
	</bean>

	<bean id="teleoperationUsername" parent="credentialPart">
		<constructor-arg value="user" />
	</bean>

	<bean id="teleoperationPassword" parent="credentialPart">
		<constructor-arg value="password" />
	</bean>

	<bean id="adminUsername" parent="credentialPart">
		<constructor-arg value="user" />
	</bean>

	<bean id="adminPassword" parent="credentialPart">
		<constructor-arg value="password" />
	</bean>
	
	<bean id="schedulerUsername" parent="credentialPart">
		<constructor-arg value="user" />
	</bean>

	<bean id="schedulerPassword" parent="credentialPart">
		<constructor-arg value="password" />
	</bean>

</beans>
